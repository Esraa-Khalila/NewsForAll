@model IEnumerable<ContactU>



<div class="container">
    <div class="row">
        <div class="col-12 m-2 font-weight-bold text-center"> All Message</div>

        
        @if (Model.Count() > 0)
        {
            <table class="table table-bordered  ">
                <thead>
                    <tr>

                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Message</th>
                        





                    </tr>
                </thead>
                <tbody>
                    @foreach (var Contact in Model)
                    {
                        <tr>

                            <td width="5%">@Contact.Fna</td>
                            <td width="5%">@Contact.Lna</td>
                            <td width="5%">@Contact.Email</td>
                            <td width="5%">@Contact.Message</td>
                  


                        </tr>
                    }

                </tbody>
            </table>
        }
        else
        {
            <h4>No Message yet</h4>
        }


    </div>
</div>



